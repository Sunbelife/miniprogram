.container {
    /*font-family: 'FZLTZHK';*/
    font-size: 34rpx;
    min-height: 100vh;
    color: #333333;
} 


.form-ele{
    padding: 30rpx 20rpx;
    background-color: white;
    margin-bottom: 20rpx;
}
.form-ele-val{
    float: right;
   color: #b0b0b0;
}


.text-center{
    text-align: center;
}

.hidden{
    display: none;
}


.flex{
    display: flex;
}



.form-item{
    display: flex;
    padding: 20rpx;
    border-bottom: 1px solid #ddd;
    position: relative;
}
.form-label{
    width: 200rpx;
    text-align: center;
}

.form-input{
    flex: 1;
}

.bottomBtn{
   position: fixed;
    bottom:0;
    width: 750rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    color: white;
    background-color: #f83146;
    border-radius: 0;
}




.full-page{
    position: fixed;
    height: 100vh;
    width: 100vw;
    bottom: 0;
    left: 0;
    /*background: rgba(0,0,0,.6);*/
    background: white;
    z-index: 20;
}

.full-page-over{
    z-index: 22;

}


.iconfont{
    display: inline-block;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
    font-family:"iconfont" !important;
    font-size:16px;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

